package com.clearexpenses.main.dtos;

public class ExpensesResponse {
	private Long id;
	private String name;
	private String mobile;
	private String needToPay;
	private float payableMoney;
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getNeedToPay() {
		return needToPay;
	}
	public void setNeedToPay(String needToPay) {
		this.needToPay = needToPay;
	}
	public float getPayableMoney() {
		return payableMoney;
	}
	public void setPayableMoney(float payableMoney) {
		this.payableMoney = payableMoney;
	}
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	
	
	

}
